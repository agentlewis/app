{
  "name": "holodex",
  "version": "0.0.0",
  "description": "People and group relationships",
  "main": "src/server.js",
  "browser": "src/client.js",
  "scripts": {
    "test-server": "tape src/node_modules/**/test-server",
    "test-client": "run-browser src/node_modules/**/test-client -b",
    "test": "npm run test-server && npm run test-client",
    "build": "browserify . -o src/assets/bundle.js",
    "watch": "watchify . -o src/assets/bundle.js",
    "live": "npm run watch & node-dev .",
    "develop": "NODE_ENV=development npm run live",
    "prod": "NODE_ENV=production npm start",
    "start": "npm run build && node .",
    "deploy": "git remote add deploy dokku@holodex.enspiral.info:holodex; git push deploy master",
    "postinstall": "npm run dedupe"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/craftodex/app.git"
  },
  "keywords": [
    "holodex",
    "holon",
    "holons",
    "rolodex",
    "directory",
    "ui",
    "api",
    "people",
    "groups"
  ],
  "author": "Enspiral Craftworks",
  "license": "AGPLv3",
  "bugs": {
    "url": "https://github.com/craftodex/app/issues"
  },
  "homepage": "https://github.com/craftodex/app",
  "browserify": {
    "transform": [
      "envify"
    ]
  },
  "dependencies": {
    "ampersand-collection": "^1.3.17",
    "ampersand-collection-lodash-mixin": "git://github.com/ahdinosaur/ampersand-collection-lodash-mixin",
    "ampersand-collection-rest-mixin": "^3.0.1",
    "ampersand-model": "^4.0.3",
    "async-each": "^0.1.6",
    "body-parser": "^1.10.1",
    "bootstrap": "^3.3.1",
    "browserify": "^9.0.3",
    "camel-case": "^1.1.1",
    "clientconfig": "^1.0.0",
    "compression": "^1.3.0",
    "config": "^1.9.0",
    "cookie-parser": "^1.3.3",
    "create-element": "~0.2.3",
    "d": "^0.1.1",
    "d3-dispatch": "0.0.0",
    "debug": "^2.1.1",
    "envify": "^3.2.0",
    "es-class": "^0.9.0",
    "es6-map": "^0.1.1",
    "express": "^4.10.7",
    "feathers": "^1.0.1",
    "for-each": "^0.3.2",
    "global": "^4.3.0",
    "helmet": "^0.5.4",
    "inherits": "^2.0.1",
    "js-yaml": "^3.2.5",
    "json-stringify-safe": "^5.0.0",
    "jsonpath": "^0.1.3",
    "less-middleware": "^1.0.4",
    "lodash": "^2.4.1",
    "lokijs": "^1.2.0",
    "map-values": "^1.0.1",
    "nets": "^2.0.0",
    "object-keys-map": "^1.0.2",
    "observ": "^0.2.0",
    "observ-location": "^1.0.0",
    "observ-struct": "^6.0.0",
    "path-match": "^1.2.2",
    "r-dom": "^1.0.2",
    "react": "^0.12.2",
    "react-bootstrap": "^0.13.0",
    "react-style": "^0.4.0",
    "reverend": "^0.3.0",
    "selfish": "^0.3.3",
    "serve-static": "^1.7.2",
    "sliced": "0.0.5",
    "traverse": "^0.6.6",
    "traverse-async": "git://github.com/ahdinosaur/js-traverse-async#build",
    "url-regex": "^2.1.2",
    "url-regexp": "^1.0.2",
    "webcola": "git://github.com/simontegg/WebCola",
    "xtend": "^4.0.0"
  },
  "devDependencies": {
    "easy-livereload": "^1.0.3",
    "node-dev": "^2.6.1",
    "run-browser": "^2.0.2",
    "tape": "^3.5.0",
    "watchify": "^2.4.0"
  }
}
