{
  "name": "holodex",
  "version": "0.0.0",
  "description": "People and group relationships",
  "main": "src/server.js",
  "browser": "src/client.js",
  "scripts": {
    "test-server": "tape src/node_modules/**/test-server",
    "test-client": "run-browser src/node_modules/**/test-client --port 3030 -b | tap-spec",
    "test": "npm run test-server && npm run test-client",
    "build": "browserify . | uglifyjs > src/assets/bundle.js",
    "watch": "watchify . -o src/assets/bundle.js",
    "live": "npm run watch & node-dev .",
    "develop": "NODE_ENV=development npm run live",
    "prod": "NODE_ENV=production npm start",
    "start": "npm run build && node .",
    "deploy": "git remote add deploy dokku@holodex.enspiral.info:holodex; git push deploy master",
    "postinstall": "npm dedupe"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/craftodex/app.git"
  },
  "keywords": [
    "holodex",
    "holon",
    "holons",
    "rolodex",
    "directory",
    "ui",
    "api",
    "people",
    "groups"
  ],
  "author": "Enspiral Craftworks",
  "license": "AGPLv3",
  "bugs": {
    "url": "https://github.com/craftodex/app/issues"
  },
  "homepage": "https://github.com/craftodex/app",
  "browserify": {
    "transform": [
      "envify"
    ]
  },
  "dependencies": {
    "ampersand-state": "^4.5.2",
    "bootstrap": "^3.3.1",
    "browserify": "^9.0.3",
    "clientconfig": "^1.0.0",
    "config": "^1.9.0",
    "cookie-parser": "^1.3.3",
    "create-element": "~0.2.3",
    "d3-dispatch": "0.0.0",
    "debug": "^2.1.1",
    "envify": "^3.2.0",
    "express": "^4.10.7",
    "fs-db": "^2.1.0",
    "got": "^2.5.0",
    "helmet": "^0.5.4",
    "js-yaml": "^3.2.5",
    "json-stringify-safe": "^5.0.0",
    "less-middleware": "^1.0.4",
    "lodash.assign": "^3.0.0",
    "lodash.clone": "^3.0.1",
    "lodash.find": "^3.2.0",
    "lodash.flatten": "^3.0.2",
    "lodash.foreach": "^3.0.2",
    "lodash.isarray": "^3.0.1",
    "lodash.map": "^3.1.0",
    "lodash.shuffle": "^3.0.0",
    "lodash.union": "^3.1.0",
    "lokijs": "^1.2.3",
    "map-values": "^1.0.1",
    "object-keys-map": "^1.0.2",
    "observ-location": "^1.0.0",
    "path-match": "^1.2.2",
    "pull-stream": "^2.26.0",
    "r-dom": "^1.0.2",
    "react": "^0.12.2",
    "react-bootstrap": "^0.13.0",
    "reverend": "^0.3.0",
    "send-data": "^6.0.0",
    "serve-static": "^1.7.2",
    "uglifyjs": "^2.4.10",
    "url-regex": "^2.1.2",
    "webcola": "git://github.com/simontegg/WebCola",
    "xtend": "^4.0.0"
  },
  "devDependencies": {
    "easy-livereload": "^1.0.3",
    "node-dev": "^2.6.1",
    "rnd": "0.0.0",
    "run-browser": "^2.0.2",
    "shmock": "^0.7.1",
    "tap-spec": "^2.2.2",
    "tape": "^3.5.0",
    "watchify": "git://github.com/holodex/watchify#1e26dc06b38601b95d88efca3b5e1e66446d25bc"
  }
}
